        <type>int</type>
        <definition>int modem_key_mgmt_write</definition>
        <argsstring>(nrf_sec_tag_t sec_tag, enum modem_key_mgmt_cred_type cred_type, const void *buf, size_t len)</argsstring>
        <name>modem_key_mgmt_write</name>
