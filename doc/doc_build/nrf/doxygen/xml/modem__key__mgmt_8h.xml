        </param>
        <param>
          <type>enum <ref refid="group__modem__key__mgmt_gaf6e72956fd99711667f2492b33ab526f_1gaf6e72956fd99711667f2492b33ab526f" kindref="member">modem_key_mgmt_cred_type</ref></type>
