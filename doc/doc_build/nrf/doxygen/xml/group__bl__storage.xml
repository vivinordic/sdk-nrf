      <memberdef kind="function" id="group__bl__storage_1ga74d02e3eeb5ad4d8a86d69438f14ff7a" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void otp_copy32</definition>
        <argsstring>(uint8_t *restrict dst, uint32_t volatile *restrict src, size_t size)</argsstring>
        <name>otp_copy32</name>
        <param>
