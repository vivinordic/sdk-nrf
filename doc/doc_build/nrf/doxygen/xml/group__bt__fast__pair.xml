      <memberdef kind="function" id="group__bt__fast__pair_1gaa3ab736b470b5650a118b1b0135bdc80" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type>size_t</type>
        <definition>size_t bt_fast_pair_adv_data_size</definition>
        <argsstring>(struct bt_fast_pair_adv_config fp_adv_config)</argsstring>
        <name>bt_fast_pair_adv_data_size</name>
        <param>
          <type>struct <ref refid="structbt__fast__pair__adv__config" kindref="compound">bt_fast_pair_adv_config</ref></type>
          <declname>fp_adv_config</declname>
        </param>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para>Get Fast Pair advertising data buffer size.</para>
<para><parameterlist kind="param"><parameteritem>
<parameternamelist>
<parametername direction="in">fp_adv_config</parametername>
</parameternamelist>
<parameterdescription>
<para>Fast Pair advertising config.</para>
</parameterdescription>
</parameteritem>
</parameterlist>
<simplesect kind="return"><para>Fast Pair advertising data buffer size in bytes if the operation was successful. Otherwise zero is returned. </para>
</simplesect>
</para>
        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
