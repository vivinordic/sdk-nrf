    <compoundname>location_module_event</compoundname>
    <title>Location module event</title>
    <innerclass refid="structlocation__module__pvt" prot="public">location_module_pvt</innerclass>
    <innerclass refid="structlocation__module__neighbor__cells" prot="public">location_module_neighbor_cells</innerclass>
    <innerclass refid="structlocation__module__cloud__location" prot="public">location_module_cloud_location</innerclass>
